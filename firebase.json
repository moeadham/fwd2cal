{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint"
      ]
    }
  ],
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "pubsub": {
      "port": 8085
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true,
    "auth": {
      "port": 9099
    }
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
    ],
    "rewrites": [
      {
        "source": "/not-found",
        "destination": "/404.html"
      },
      {
        "source": "/auth/verifyAdditionalEmail",
        "function": "verifyAdditionalEmail"
      },
      {
        "source": "/auth/oauthCallback",
        "function": "oauthCallback"
      },
      {
        "source": "/signup",
        "function": "signup"
      }
    ]
  }
}
